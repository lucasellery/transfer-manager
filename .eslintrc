{
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
  },
  "eslintConfig": {
    "extends": ["airbnb", "prettier", "plugin:jsx-a11y/recommended"],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src", "@/"],
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      }
    }
  }
}